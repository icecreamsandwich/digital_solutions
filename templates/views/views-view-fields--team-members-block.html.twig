{% set image_uri = fields.field_team_member_image.entity.uri %}
{% set url = file_url(content.field_team_member_image['#items'].entity.uri) %}
<div class="col-lg-3 col-md-6 d-flex align-items-stretch">
    <div class="member" data-aos="fade-up" data-aos-delay="100">
        <div class="member-img">
            <img src="{{ base_path ~ directory }}/images/team/team-1.jpg" class="img-fluid" alt="">
            <div class="social">
                <!-- {% for links in content['#view'].field['field_social_media_links'].value(row) %}
                            <a href=""><i class="bi bi-facebook"></i></a>
                        {% endfor %} -->
                <a href=""><i class="bi bi-twitter"></i></a>
                <a href=""><i class="bi bi-facebook"></i></a>
                <a href=""><i class="bi bi-instagram"></i></a>
                <a href=""><i class="bi bi-linkedin"></i></a>
            </div>
        </div>
        <div class="member-info">
            <h4>{{ fields.field_team_member_name.content }}</h4>
            <span>{{ fields.field_member_designation.content }}</span>
        </div>
    </div>
</div>

